<!DOCTYPE html>
<html>
  <%- include('partials/_head.ejs') -%>

  <body>
    <%- include('partials/_navigation.ejs') -%>
    <!-- header section end -->
    <!-- contact section start -->
    <div class="contact_section layout_padding">
      <div class="container">
        <div class="row">
          <div class="col-sm-12">
            <h1 class="contact_taital contact_heading">Login</h1>
          </div>
        </div>
        <div class="contact_section_2">
          <div class="row">
            <div class="col-md-12">
              <div class="mail_section map_form_container">
                <form action="/users/login" method="post">
                  <input
                    type="text"
                    class="mail_text"
                    placeholder="Email"
                    name="email"
                  />
                  <input
                    type="text"
                    class="mail_text"
                    placeholder="Password"
                    name="password"
                  />

                  <div class="btn_main">
                    <div class="send_bt">
                      <button class="" type="submit">Login</button>
                    </div>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- contact section end -->
    <%- include('partials/_footer.ejs') -%>
  </body>
</html>
