<!DOCTYPE html>
<html lang="en">
  <%- include('partials/_head') -%>
  <!-- body -->
  <body class="main-layout">
    <!-- loader  -->
    <%- include('partials/_loader') -%>
    <!-- end loader -->
    <!-- header -->
    <%- include('partials/_navigation') -%>
    <!-- end header inner -->
    <!-- end header -->
    <div class="back_re">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <div class="title">
              <h2>REGISTER</h2>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!--  contact -->
    <div id="contact" class="contact">
      <div class="container">
        <div class="row">
          <div class="offset-3 col-6">
            <form class="main_form" method="post" action="/users/register">
              <div class="row">
                <div class="col-md-12">
                  <input
                    class="contactus"
                    placeholder="Email"
                    type="email"
                    name="email"
                  />
                </div>
                <div class="col-md-12">
                  <input
                    class="contactus"
                    placeholder="Password"
                    type="password"
                    name="password"
                  />
                </div>
                <div class="col-md-12">
                  <button class="send_btn">Register</button>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
    <!-- end contact -->
    <!--  footer -->
    <%- include('partials/_footer') -%>
  </body>
</html>
