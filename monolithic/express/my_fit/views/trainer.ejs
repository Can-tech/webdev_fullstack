<!DOCTYPE html>
<html lang="en">
  <%- include('partials/_head') -%>
  <!-- body -->
  <body class="main-layout">
    <!-- loader  -->
    <%- include('partials/_loader') -%>
    <!-- end loader -->
    <!-- header -->
    <%- include('partials/_navigation') -%>
    <!-- end header inner -->
    <!-- end header -->
    <div class="back_re">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <div class="title">
              <h2>Our Trainers</h2>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- trainers -->
    <div class="trainers">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <div class="titlepage">
              <p>Lorem Ipsum available, but the majority have suffered</p>
            </div>
          </div>
        </div>
        <div class="row">
          <% instructors.forEach(e=>{ %>
          <div class="col-md-4">
            <div id="trai" class="trainer_box">
              <div class="trainer_img">
                <figure><img src="/images/tra.jpg" alt="#" /></figure>
              </div>
              <div class="trainer">
                <h3><%=e.email%></h3>
                <p>Trainer</p>
              </div>
            </div>
          </div>
          <%})%>
        </div>
      </div>
    </div>
    <!-- end trainers -->
    <!--  footer -->
    <%- include('partials/_footer') -%>
  </body>
</html>
