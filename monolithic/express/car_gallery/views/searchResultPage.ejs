<!DOCTYPE html>
<html class="no-js" lang="en">
  <%- include('partials/_head') -%>

  <body>
    <!--featured-cars start -->
    <%- include('partials/_navigation') -%>
    <section id="featured-cars" class="featured-cars" style="margin-top: 20px">
      <div class="container">
        <div class="section-header">
          <p>checkout <span>the</span> search result</p>
          <h2>found cars</h2>
        </div>
        <!--/.section-header-->
        <div class="featured-cars-content-search-page">
          <% cars.forEach(car => {%>
          <div class="col-lg-3 col-md-4 col-sm-6">
            <div class="single-featured-cars">
              <div class="featured-img-box">
                <div class="featured-cars-img">
                  <img src="./images/featured-cars/fc1.png" alt="cars" />
                </div>
                <div class="featured-model-info">
                  <p>
                    model: <%= car.year%>
                    <span class="featured-hp-span"
                      >HP: <%= car.horsepower%></span
                    >
                    make: <%= car.make%>
                    <span class="featured-hp-span"
                      >Condition: <%= car.condition%></span
                    >
                    body: <%= car.bodyStyle%>
                    <span class="featured-hp-span">Color: <%= car.color%></span>
                  </p>
                </div>
              </div>
              <div class="featured-cars-txt">
                <h2><a href="#"><%= car.name %></a></h2>
                <h3>$<%= Number(car.price).toLocaleString('en-US')%></h3>
                <p><%= car.description %></p>
              </div>
            </div>
          </div>
          <%})%>
        </div>
      </div>
      <!--/.container-->
    </section>

    <%- include('partials/_footer') -%>
  </body>
</html>
